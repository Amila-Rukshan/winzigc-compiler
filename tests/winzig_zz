{ This program tests simple function calls for many argument. }
program MyProg:

var 
    i, j : integer; 
    b, k : boolean;

begin
    read(i);
    read(j);
    output(i, j);
    output(i + j);
    i := 15 - 3 * i;
    j := i + 7;
    j := 2 * i + j;
    output(i, j);
    output(i);
end 

MyProg.
